{
    "name": "nrawe/june",
    "description": "A small, simple TDD framework for PHP",
    "type": "app",
    "require": {
        "league/climate": "^3.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Rawe",
            "homepage": "https://github.com/nrawe"
        }
    ],
    "autoload": {
        "psr-4": {
            "June\\Framework\\": "library/"
        },
        "files": [
            "library/helpers.php",
            "library/dsl.php"
        ]
    },
    "minimum-stability": "stable",
    "suggest": {
        "psy/psysh": "If you want to use the June\\debug() helper"
    },
    "require-dev": {
        "psy/psysh": "^0.8.7"
    }
}
